<section class="jumbotron">
  <div class="video">
    <% if (theme.ocean.overlay) { %>
      <div class="video-frame">
        <img src="<%- theme.ocean.path %>overlay-hero.png" alt="Decorative image frame">
      </div>
    <% } %>
    <div class="video-media">
      <% if (theme.ocean.video) { %>
        <video playsinline="" autoplay="" loop="" muted="" data-autoplay=""
             poster="<%- theme.ocean.videoPath %>ocean.png" x5-video-player-type="h5">
          <source src="<%- theme.ocean.videoPath %>ocean.mp4" type="video/mp4">
          <source src="<%- theme.ocean.videoPath %>ocean.ogv" type="video/ogg">
          <source src="<%- theme.ocean.videoPath %>ocean.webm" type="video/webm">
          <p>Your user agent does not support the HTML5 Video element.</p>
        </video>
      <% } %>
      <% if (theme.ocean.image) { %>
        <img src="<%- theme.ocean.bgImage %>" alt="background">
      <% } %>
      <div class="video-overlay"></div>
    </div>
    <div class="video-inner text-center text-white">
      <h1><a href="<%- url_for() %>"><%= config.title %></a></h1>
      <p><%= config.subtitle %></p>
      <div><img src="<%- theme.ocean.brand %>" class="brand" alt="<%= config.title %>"></div>
    </div>
    <div class="video-learn-more">
      <a class="anchor" href="#landingpage"><i class="fe fe-mouse"></i></a>
    </div>
  </div>
</section>
